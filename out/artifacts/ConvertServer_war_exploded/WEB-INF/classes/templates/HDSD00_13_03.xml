                <low th:attr="value=${cdaData.customCol.DE06_00_092_00}"/>
                <!-- 出院日期时间 -->
                <high th:attr="value=${cdaData.customCol.DE06_00_017_00}"/>
            </effectiveTime>
        </encompassingEncounter>
    </componentOf>
