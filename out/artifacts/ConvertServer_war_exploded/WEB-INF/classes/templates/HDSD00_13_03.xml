                <name  th:text="${cdaData.customCol.DE08_10_052_00_NAME}"></name>
            </representedCustodianOrganization>
        </assignedCustodian>
    </custodian>
    <!-- 最终审核者签名 -->
    <legalAuthenticator>
        <time/>
        <signatureCode/>
        <assignedEntity>
            <id root="2.16.156.10011.1.4"  />
            <code displayName="主任医师"/>
            <assignedPerson>
                <name th:text="${cdaData.customCol.DE02_01_039_00_ZRYS}"></name>
            </assignedPerson>
        </assignedEntity>
    </legalAuthenticator>
    <!-- 接诊医师签名 -->
    <authenticator>
        <time/>
        <signatureCode/>
        <assignedEntity>
            <id root="2.16.156.10011.1.4"  />
            <code displayName="接诊医师"/>
            <assignedPerson>
                <name th:text="${cdaData.customCol.DE02_01_039_00_JZYS}"></name>
            </assignedPerson>
        </assignedEntity>
    </authenticator>
    <!--住院医师签名-->
    <authenticator>
        <time/>
        <signatureCode/>
        <assignedEntity>
            <id root="2.16.156.10011.1.4"  />
            <code displayName="住院医师"/>
            <assignedPerson>
                <name th:text="${cdaData.customCol.DE02_01_039_00_ZYYS}"></name>
            </assignedPerson>
        </assignedEntity>
    </authenticator>
    <!--主治医师签名-->
    <authenticator>
        <time/>
        <signatureCode/>
        <assignedEntity>
            <id root="2.16.156.10011.1.4" />
            <code displayName="主治医师"/>
            <assignedPerson>
                <name th:text="${cdaData.customCol.DE02_01_039_00_ZZYS}"></name>
            </assignedPerson>
        </assignedEntity>
